<nav>
  {{> main-logo }}
  {{> current-time }}
</nav>
<section class="room-list">
  {{#each filteredData}}
      <article class="room-list__card">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 535.47 293.19">
            {{> svgStyle }}
            {{> background }}
            {{> chairs}}
            {{> occupation }}
            {{> desk }}
            {{> lighting}}
          </svg>
          <section class="room-list__visual-data">
              <p>{{ this.roomTemp.celcius }} &#8451;</p>
              <p> {{ this.decibels }}</p>
          </section>
          <a href="room/{{ this.roomName }}" class="room-list__button" type="button" name="button">{{ this.roomName }}</a>
      </article>
  {{/each}}
</section>
